{
  "name": "boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "yarn prestorybook && patch-package",
    "reactotron": "adb reverse tcp:9090 tcp:9090",
    "android": "react-native run-android && yarn reactotron",
    "ios": "react-native run-ios",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --max-warnings 0",
    "build:number:up": "node scripts/incBuildNumber.js",
    "version:set": "node scripts/setVersion.js",
    "version:up": "node scripts/incVersion.js",
    "merge:dev:stage": "git checkout stage && git pull && git merge origin/develop -m \"Merge develop to stage\" && git push && git checkout develop",
    "merge:stage:master": "git checkout master && git pull && git merge origin/stage -m \"Merge stage to master\" && git push && git checkout develop",
    "testflight": "cd ios && bundle install && bundle exec fastlane beta",
    "playalpha": "cd android && bundle install && bundle exec fastlane beta",
    "detox:ios": "yarn detox:build:ios && yarn detox:test:ios",
    "detox:build:ios": "detox build --configuration ios",
    "detox:test:ios": "detox test --configuration ios",
    "clear": "cd android && ./gradlew clean && cd .. && rm -rf node_modules ios/build android/build android/app/build ios/Pods ios/Podfile.lock yarn.lock && yarn && npx pod-install"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": "./src",
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.3.2",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^14.11.0",
    "@react-native-firebase/auth": "^14.11.0",
    "@react-native-firebase/crashlytics": "^14.11.0",
    "@react-native-firebase/firestore": "^14.11.0",
    "@react-native-firebase/perf": "^14.11.0",
    "@react-native-firebase/storage": "^14.11.0",
    "@react-navigation/core": "^6.2.1",
    "@react-navigation/elements": "^1.3.3",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "@types/styled-components": "^5.1.25",
    "axios": "^0.27.2",
    "color": "^4.2.3",
    "filesize": "^9.0.7",
    "final-form": "^4.20.7",
    "humanize-duration": "^3.27.2",
    "i18next": "^21.8.10",
    "iso-639-1": "^2.1.15",
    "libphonenumber-js": "^1.10.6",
    "lodash": "^4.17.21",
    "mobx": "^6.6.0",
    "mobx-react-lite": "^3.4.0",
    "mobx-state-tree": "^5.1.5",
    "mobx-store-provider": "^2.1.2",
    "moment": "^2.29.3",
    "mst-middlewares": "^5.1.5",
    "mst-persist": "^0.1.3",
    "prop-types": "^15.8.1",
    "react": "17.0.1",
    "react-final-form": "^6.5.9",
    "react-i18next": "^11.17.2",
    "react-native": "^0.64.2",
    "react-native-config": "^1.4.6",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-gifted-chat": "^1.0.2",
    "react-native-hooks": "^0.9.0",
    "react-native-image-crop-picker": "^0.37.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-lightbox": "^0.8.1",
    "react-native-localize": "^2.2.2",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^4.12.1",
    "react-native-permissions": "^3.3.1",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.13.1",
    "react-native-vector-icons": "^9.1.0",
    "reactotron-mst": "^3.1.4",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
